{
  "name": "cost-trackingapp",
  "version": "1.0.0",
  "description": "MERN cost tracking app GcivilCon",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanjaya1996/greencivilcon-costTrackingApp.git"
  },
  "author": "Sanjay Dahal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sanjaya1996/greencivilcon-costTrackingApp/issues"
  },
  "homepage": "https://github.com/sanjaya1996/greencivilcon-costTrackingApp#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.12"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
